    {
      "id": "resume-screener",
      "name": "Resume Screener Agent",
      "category": "hr",
      "version": "2.0.0",
      "icon": "üìã",
      "description": "Screens resumes against job requirements, scores candidates, and identifies top matches while ensuring fair evaluation.",
      "capabilities": ["Resume Parsing", "Skills Matching", "Bias Detection", "ATS Integration"],
      "setupTime": "15 min",
      "rating": 4.8,
      "deployments": 2800,
      "config": {
        "parsing": {
          "formats": ["pdf", "docx", "txt"],
          "extractFields": ["contact", "experience", "education", "skills", "certifications"]
        },
        "matching": {
          "weights": {
            "requiredSkills": 0.40,
            "experience": 0.30,
            "education": 0.15,
            "certifications": 0.15
          },
          "skillSynonyms": true,
          "experienceEquivalents": true
        },
        "fairness": {
          "blindScreening": true,
          "removeFields": ["name", "photo", "age", "gender"],
          "diversityFlags": false
        },
        "output": {
          "scoreCard": true,
          "topCandidates": 10,
          "rejectReasons": true,
          "exportFormat": "csv"
        }
      },
      "integrations": ["greenhouse", "lever", "workday", "bamboohr"]
    },
    {
      "id": "data-entry",
      "name": "Data Entry Agent",
      "category": "operations",
      "version": "2.5.0",
      "icon": "‚å®Ô∏è",
      "description": "Extracts data from documents, emails, and forms. Validates, transforms, and loads into your systems with 99% accuracy.",
      "capabilities": ["OCR/Extraction", "Validation", "Transformation", "API Integration"],
      "setupTime": "20 min",
      "rating": 4.9,
      "deployments": 3500,
      "config": {
        "sources": {
          "email": { "enabled": true, "inbox": "documents@company.com" },
          "uploads": { "enabled": true, "folder": "/incoming" },
