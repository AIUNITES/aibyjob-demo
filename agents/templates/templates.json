        "validation": {
          "rules": [
            { "field": "amount", "type": "number", "min": 0 },
            { "field": "date", "type": "date", "format": "YYYY-MM-DD" },
            { "field": "email", "type": "email" }
          ],
          "humanReview": {
            "confidence": 0.85,
            "queue": "review-queue"
          }
        },
        "destination": {
          "type": "api",
          "endpoint": "https://api.company.com/data",
          "format": "json",
          "batch": false
        }
      },
      "integrations": ["google-vision", "aws-textract", "airtable", "google-sheets", "zapier"]
